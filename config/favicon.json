{
    "master_picture": "TODO: Path to your master picture",
    "favicon_design": {
        "ios": {
            "picture_aspect": "no_change",
            "assets": {
                "ios6_and_prior_icons": true,
                "ios7_and_later_icons": true,
                "precomposed_icons": true,
                "declare_only_default_icon": true
            },
            "app_name": "Foxcraft Wiki"
        },
        "desktop_browser": {
            "design": "raw"
        },
        "windows": {
            "picture_aspect": "no_change",
            "background_color": "#da532c",
            "on_conflict": "override",
            "assets": {
                "windows_80_ie_10_tile": true,
                "windows_10_ie_11_edge_tiles": {
                    "small": true,
                    "medium": true,
                    "big": true,
                    "rectangle": true
                }
            },
            "app_name": "Foxcraft Wiki"
        },
        "android_chrome": {
            "picture_aspect": "no_change",
            "theme_color": "#ac5900",
            "manifest": {
                "name": "Foxcraft Wiki",
                "start_url": "https:\/\/wiki.foxcraft.biz",
                "display": "standalone",
                "orientation": "not_set",
                "on_conflict": "override",
                "declared": true
            },
            "assets": {
                "legacy_icon": true,
                "low_resolution_icons": true
            }
        },
        "safari_pinned_tab": {
            "picture_aspect": "black_and_white",
            "threshold": 40,
            "theme_color": "#a84600"
        }
    },
    "settings": {
        "compression": 1,
        "scaling_algorithm": "Mitchell",
        "error_on_image_too_small": false,
        "readme_file": true,
        "html_code_file": true,
        "use_path_as_is": false
    }
}